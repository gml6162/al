#include<stdio.h>

int main()
{
	int group[4]={0};
	char can;
	printf("a,b,c,d 중에 입력하시오.");
	while(1)
	{
		scanf("%c", &can);
		if(can=='z')
		break;
		switch(can)
		{
			case 'a':
			++group[0];
			break;
			case 'b':
			++group[1];
			break;
			case 'c':
			++group[2];
			break;
			case 'd':
			++group[3];
			break;
			default :
			break;
		}
		
	}
	printf("%d %d %d %d", group[0], group[1], group[2], group[3]);
	return 0;

}
